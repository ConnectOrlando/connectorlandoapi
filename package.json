{
  "name": "connectorlandoapi",
  "version": "1.0.0",
  "description": "api",
  "repository": "https://github.com/ConnectOrlando/connectorlandoapi.git",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon -w .env -w . server.js",
    "debug": "node --inspect-brk server.js",
    "lint": "eslint --ext=.js --ignore-path=.gitignore src",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier --write --ignore-path=.gitignore \"src/**/*.{js,jsx}\""
  },
  "author": "Laura Boettcher, Pablo Najera",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.11.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "lint-staged": "^13.1.1",
    "nodemon": "^2.0.20",
    "prettier": "2.8.0",
    "prisma": "^4.11.0",
    "simple-git-hooks": "2.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "yarn run format"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix"
  }
}
